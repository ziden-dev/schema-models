{
  "@name": "us-document-id",
  "@type": "context",
  "@id": "usdi",
  "@context": [
    "https://raw.githubusercontent.com/ziden-dev/schema-models/main/json/us-fips-subdivision-code.json"
  ],
  "doc-type": {
    "@type": "std:int",
    "@values": [0, 1],
    "@display": ["Passport", "Identity Card"]
  },
  "address": {
    "@type": "std:obj",
    "street": {
      "@type": "std:str",
      "@values": []
    },
    "county": {
      "@type": "fips-usc:county-code",
      "@values": []
    },
    "state": {
      "@type": "fips-usc:state-code",
      "@values": []
    }
  },
  "gender": {
    "@type": "std:int",
    "@values": [0, 1, 2],
    "@display": ["male", "female", "others"]
  },
  "id-number": {
    "@type": "std:str",
    "@values": []
  }
}
